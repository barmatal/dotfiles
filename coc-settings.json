{
  "snippets.ultisnips.enable": true,
  "snippets.snipmate.enable": true,
  "coc.source.around.enable": false,
  "coc.source.buffer.enable": false,
  "coc.preferences.formatOnSaveFiletypes": ["json"],
  "coc.preferences.extensionUpdateCheck": "daily",
  "codeLens.enable": true,
  "diagnostic.virtualText": true,
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "suggest.floatEnable": true,
  "suggest.noselect": false,
  "suggest.minTriggerInputLength": 1,
  "snippets.extends": {
    "javascript": [
      "javascript",
      "javascript-jasmine-arrow",
      "javascript-jsdoc"
    ],
    "typescript": [
      "javascript",
      "javascript-jasmine-arrow",
      "javascript-jsdoc"
    ],
    "vimwiki": ["markdown"],
    "ledger": []
  }
}
